cctools_home=/usr/local/cctools

echo Adding $cctools_home/bin to PATH
export PATH=${cctools_home}/bin:${PATH}

echo Adding $cctools_home/lib/python2.7/site-packated to PYTHONPATH
export PYTHONPATH=${cctools_home}/lib/python2.7/site-packages

echo Setting work queue port range to 10000-10999
export TCP_LOW_PORT=10000
export TCP_HIGH_PORT=10999

