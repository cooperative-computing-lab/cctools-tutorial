# Tell python where to find work queue and the workers

$ PATH=${HOME}/cctools-tutorial/cctools/bin:${PATH}
$ export PATH

$ PYTHONPATH=${HOME}/cctools-tutorial/cctools/lib/python2.7/site-packages:${PYTHONPATH}
$ export PYTHONPATH

$ python example_02.py
